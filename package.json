{
  "name": "KenangaMobile",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --config ./.eslintrc.json --ext .js,.jsx,.ts,.tsx",
    "format_all": "prettier --write \"{src,tests}/**/*.{tsx,ts,json,js,jsx}\"",
    "build": "npm run tsc",
    "build:android:debug": "cd android && ./gradlew assembleDebug",
    "build:android:release": "cd android && ./gradlew assembleRelease",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/KenangaMobile/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'",
    "clean:android": "cd android/app && rm -rf build",
    "clean:pods": "cd ios && rm -rf pods && pod install",
    "clean:ios": "npm run clean:pods && cd ios && rm -rf build",
    "clean:modules": "rm -rf node_modules && yarn",
    "install:pods": "cd ios && pod install",
    "remove:ios:build": "cd ios && rm -rf build",
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator=\"iPad (7th generation)\"",
    "ios:device": "react-native run-ios --device",
    "ios:9": "react-native run-ios --simulator=\"iPad Pro (9.7-inch)\"",
    "ios:10": "react-native run-ios --simulator=\"iPad (7th generation)\"",
    "ios:11": "react-native run-ios --simulator=\"iPad Pro (11-inch) (2nd generation)\"",
    "ios:12": "react-native run-ios --simulator=\"iPad Pro (12.9-inch) (4th generation)\"",
    "fix:collapsible:js": "cp ./fixes/react-native-collapsible--collapsible ./node_modules/react-native-collapsible/Collapsible.js",
    "fix:collapsible:accordion": "cp ./fixes/react-native-collapsible--accordion ./node_modules/react-native-collapsible/Accordion.js",
    "fix:collapsible": "npm run fix:collapsible:js && npm run fix:collapsible:accordion",
    "amplify:add:codegen": "amplify codegen remove",
    "amplify:remove:codegen": "amplify add codegen --apiId rywhnkwdhfadriy4tzrfiv65xu",
    "amplify:codegen": "npm run amplify:add:codegen && npm run amplify:remove:codegen",
    "bump-patch": "npm version patch --no-git-tag-version && bundle exec fastlane bump",
    "bump-minor": "npm version minor --no-git-tag-version && bundle exec fastlane bump",
    "bump-major": "npm version major --no-git-tag-version && bundle exec fastlane bump"
  },
  "dependencies": {
    "@miblanchard/react-native-slider": "1.2.2",
    "@react-native-community/async-storage": "1.11.0",
    "@react-native-community/datetimepicker": "2.6.1",
    "@react-native-community/masked-view": "0.1.10",
    "@react-native-community/netinfo": "5.9.6",
    "@react-native-firebase/app": "8.4.5",
    "@react-navigation/native": "5.5.0",
    "@react-navigation/stack": "5.4.1",
    "amazon-cognito-identity-js": "4.4.0",
    "aws-amplify": "3.3.1",
    "aws-amplify-react-native": "4.2.6",
    "aws-appsync": "4.0.1",
    "axios": "0.20.0",
    "graphql-tag": "2.11.0",
    "base-64": "0.1.0",
    "buffer": "5.6.0",
    "moment": "2.27.0",
    "pdf-lib": "1.10.0",
    "react": "16.11.0",
    "react-native": "0.62.2",
    "react-native-chart-kit": "6.6.0",
    "react-native-collapsible": "1.5.2",
    "react-native-crypto-js": "1.0.0",
    "react-native-document-picker": "3.5.4",
    "react-native-enhanced-popup-menu": "0.7.0",
    "react-native-fs": "2.16.6",
    "react-native-gesture-handler": "1.6.1",
    "react-native-image-crop-picker": "0.32.0",
    "react-native-modal": "11.5.6",
    "react-native-reanimated": "1.9.0",
    "react-native-safe-area-context": "3.0.2",
    "react-native-screens": "2.8.0",
    "react-native-share": "3.7.0",
    "react-native-signature-capture": "0.4.10",
    "react-native-splash-screen": "3.2.0",
    "react-native-svg": "12.1.0",
    "react-native-vector-icons": "7.1.0",
    "react-native-view-pdf": "0.11.0",
    "react-native-walkthrough-tooltip": "1.1.11",
    "react-redux": "7.2.1",
    "redux": "4.0.5",
    "redux-devtools-extension": "2.13.8",
    "redux-thunk": "2.3.0",
    "rn-fetch-blob": "0.12.0"
  },
  "devDependencies": {
    "@babel/core": "7.6.2",
    "@babel/runtime": "7.6.2",
    "@react-native-community/eslint-config": "1.0.0",
    "@types/jest": "24.0.24",
    "@types/react-native": "0.62.0",
    "@types/react-native-material-dropdown": "0.11.4",
    "@types/react-native-share": "3.3.0",
    "@types/react-native-signature-capture": "0.4.1",
    "@types/react-native-vector-icons": "6.4.5",
    "@types/react-redux": "7.1.9",
    "@types/react-test-renderer": "16.9.2",
    "@types/redux": "3.6.0",
    "@types/redux-devtools-extension": "2.13.2",
    "@types/redux-thunk": "2.1.0",
    "@typescript-eslint/eslint-plugin": "2.27.0",
    "@typescript-eslint/parser": "2.27.0",
    "babel-jest": "24.9.0",
    "eslint": "6.5.1",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.0.4",
    "eslint-plugin-react-native": "3.8.1",
    "husky": "4.2.5",
    "jest": "24.9.0",
    "metro-react-native-babel-preset": "0.58.0",
    "prettier": "2.0.4",
    "react-test-renderer": "16.11.0",
    "typescript": "3.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm run format_all"
    }
  }
}
